---
import MainLayout from "../layouts/MainLayout.astro";
import Button from "../components/Button.astro";
import Link from "../components/Link.astro";
import { Image } from "astro:assets";
import thumbsup from "../assets/images/thumbsup.png";
import TrustpilotCard from "../components/TrustpilotCard.astro";
---

<MainLayout title="Titel">
  <article>
    <h1 class="bg-orange-500 mt-11xl font-display">Index overskrift</h1>
    <Button class="">Klik!</Button>
    <p>Link til <Link href="https://www.kea.dk" target="_blank">En anden side</Link></p>
    <Image src={thumbsup} alt="" />
  </article>
  <TrustpilotCard />
  <Button type="primary">Primary Button</Button>
  <Button type="secondary">Secondary Button</Button>
</MainLayout>
